    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js",
    "lint": "eslint config migrations models routes *.js",
    "lint:fix": "eslint config migrations models routes *.js --fix"
  },
  "dependencies": {
